--抢庄时间的倒计时
function  niuniu_ui:CountDownByBanker(time)
	local totalTime = require("logic.niuniu_sys.cmd_manage.niuniu_data_manage"):GetInstance().AskRobbankerData.timeo
	if totalTime ~= nil then
		local percent = tonumber(time) / tonumber(totalTime)
		if self.compTbl.countDownSliderSprite ~= nil then 
			self.compTbl.countDownSliderSprite.fillAmount = tonumber(percent)
			self.compTbl.countDownTimeLabel.text = tostring(time)
		end
	end
end

