local _xiaopaoTime = 0
local xiaopaoTimer_Elapse = nil
local xiaopaoCallBack = nil
--设置水庄倒计时
function  niuniu_ui:SetXiaoPao(time,callback)

	if time==nil or time<=0 then
		self:ShowXiaoPaoPanel(false)
		self:IsShowCountDownSlider(false)
	else
		self:SetCountDown(self.compTbl.countDownSlider.gameObject,time,slot(self.CountDownByXiaZhu,self),callback)
	end	
end

function  niuniu_ui:SetAskPoenCard(time,callback)

	if time==nil or time<=0 then
		self:ShowXiaoPaoPanel(false)
		self:IsShowCountDownSlider(false)
	else
		self:SetCountDown(self.compTbl.countDownSlider.gameObject,time,function(time)
			local totalTime = require("logic.niuniu_sys.cmd_manage.niuniu_data_manage"):GetInstance().OnAskOpenCardData.timeo
			if totalTime ~= nil then
			local percent = tonumber(time) / tonumber(totalTime)
			if self.compTbl.countDownSliderSprite ~= nil then 
				self.compTbl.countDownSliderSprite.fillAmount = tonumber(percent)
				self.compTbl.countDownTimeLabel.text = tostring(time)
			end
	end
		end,callback)
	end	
end

function  niuniu_ui:StopCountDownTimer()
	if xiaopaoTimer_Elapse ~= nil then
		xiaopaoTimer_Elapse:Stop()
		xiaopaoTimer_Elapse = nil
	end
end

