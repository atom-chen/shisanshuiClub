--抢庄时间的倒计时
function  sangong_ui:CountDownByBanker(time)
	local totalTime = require("logic.poker_sys.sangong_sys.cmd_manage.sangong_data_manage"):GetInstance().AskRobbankerData.timeo
	if totalTime ~= nil then
		local percent = tonumber(time) / tonumber(totalTime)
		if self.compTbl.countDownSliderSprite ~= nil then 
			self.compTbl.countDownSliderSprite.fillAmount = tonumber(percent)
			self.compTbl.countDownTimeLabel.text = tostring(time)
		end
		if time == 5 and not isDone then
			Notifier.dispatchCmd(cmdName.MSG_SHAKE,{})
		end
	end
end

