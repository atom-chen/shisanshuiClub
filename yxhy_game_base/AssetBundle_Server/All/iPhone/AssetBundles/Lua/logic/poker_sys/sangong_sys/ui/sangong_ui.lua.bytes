	self.compTbl.countDownTimeLabel = componentGet(child(self.widgetTbl.panel,"Anchor_Center/countdownslider/timeLbl"),"UILabel")
	self.compTbl.countDownSlider.gameObject:SetActive(false)

	self:InitMoreBtnsView()
end

