end

function  sangong_ui:SetAskPoenCard(time,callback)

	if time==nil or time<=0 then
		self:ShowXiaoPaoPanel(false)
		self:IsShowCountDownSlider(false)
	else
		self:SetSelfDone(false)
		self:SetCountDown(self.compTbl.countDownSlider.gameObject,time,function(time)
			local totalTime = require("logic.poker_sys.sangong_sys.cmd_manage.sangong_data_manage"):GetInstance().OnAskOpenCardData.timeo
